<div>
  <ul class="imglykit-controls-list imgly-controls-list-with-buttons">
    {{ for(var identifier in it.ratios) { }}
      {{ var ratio = it.ratios[identifier]; }}
      {{ var enabled = ratio.selected; }}
      <li data-identifier="{{= identifier}}" data-ratio="{{= ratio.ratio}}"{{? enabled}} data-selected{{?}}>
        <img src="{{=it.helpers.assetPath('ui/night/crop/' + identifier + '.png')}}" />
        <div class="imglykit-controls-label">{{=it.helpers.translate('operations.crop.children.' + identifier)}}</div>
      </li>
    {{ } }}
  </ul>
</div>

{{=it.partials.doneButton}}
